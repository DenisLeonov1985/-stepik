  "author": "OpenClaw Team",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^5.9.0",
    "airtable": "^0.12.2",
    "better-sqlite3": "^9.4.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "node-telegram-bot-api": "^0.64.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.11.16",
    "@types/node-telegram-bot-api": "^0.64.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
